install.packages('tidytext')
?unnest_tok
test = read.csv('user_reviews.csv')
test$test_char = as.character(test$text)
test %>% 
  unnest_tokens(word, txt, token = stringr::str_split, pattern = " ")

test %>% 
  select(grade,text) %>% 
  unnest_tokens(word, text, token = stringr::str_split, pattern = " ")
